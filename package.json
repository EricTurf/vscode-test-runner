{
  "name": "test-runner",
  "displayName": "test-runner",
  "description": "Runs test of current file",
  "version": "1.0.2",
  "publisher": "ericTurf",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.testFile"
  ],
  "main": "./extension/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.testFile",
        "title": "Test current file"
      }
    ],
    "keybindings": [
      {
        "key": "alt+cmd+t",
        "command": "extension.testFile"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EricTurf/vscode-test-runner.git"
  },
  "icon": "assets/logo.png",
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "prettier": "^1.14.3",
    "vscode": "^1.1.22"
  },
  "dependencies": {
    "rxjs": "^6.3.3"
  }
}
