{
  "name": "test-runner",
  "displayName": "test-runner",
  "description": "Runs test of current file",
  "version": "0.0.1",
  "publisher": "ericTurf",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.testFile"
  ],
  "main": "./extension/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.testFile",
        "title": "Test current file"
      }
    ],
    "keybindings": [
      {
        "key": "alt+cmd+t",
        "command": "extension.testFile"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "prettier": "^1.14.3",
    "vscode": "^1.1.21"
  },
  "dependencies": {}
}
